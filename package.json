{
  "name": "circles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "deploy": "git pull && vite build && rm -rf /var/www/circles/* && mv docs/* /var/www/circles",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook"
  },
  "dependencies": {
    "axios": "latest",
    "register-service-worker": "latest",
    "socket.io-client": "latest",
    "vue": "latest",
    "vue-class-component": "next",
    "vue-gtag-next": "latest",
    "vue-router": "latest",
    "vue-socket.io-extended": "latest",
    "vuedraggable": "latest",
    "vuex": "latest"
  },
  "devDependencies": {
    "@storybook/addon-actions": "latest",
    "@storybook/addon-knobs": "latest",
    "@storybook/addon-links": "latest",
    "@storybook/addon-notes": "latest",
    "@storybook/builder-vite": "latest",
    "@storybook/vue": "latest",
    "@types/node": "^17.0.31",
    "@vitejs/plugin-vue": "^2.3.1",
    "prettier": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-plugin-pwa": "^0.12.0",
    "vue-tsc": "latest"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "packageManager": "yarn@3.2.0"
}
